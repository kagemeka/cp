{
  "sourcekit-lsp.serverPath": "/usr/local/swift/usr/bin/sourcekit-lsp",
  "sourcekit-lsp.toolchainPath": "/usr/local/swift",
  "sourcekit-lsp.trace.server": "verbose",
  // "go.inferGopath": false,

  "files.watcherExclude": {
    "**/target": true
  },
  "files.associations": {
    "functional": "cpp",
    "vector": "cpp",
    "*.tcc": "cpp",
    "cmath": "cpp",
    "random": "cpp",
    "regex": "cpp",
    "type_traits": "cpp"
  },
}